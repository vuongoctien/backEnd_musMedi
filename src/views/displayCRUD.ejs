<!DOCTYPE html>
<html>
<style>
    table,
    th,
    td {
        border: 1px solid black;
        border-collapse: collapse;
        text-align: center;
    }
</style>

<body>
    <table style="width:100%">
        <tr>
            <th>Email</th>
            <th>firts name </th>
            <th>last name</th>
            <th>Address</th>
            <th>action</th>
        </tr>
        <% for(let i=0; i < dataTable.length; i++) { %>
            <tr>
                <td>
                    <%= dataTable[i].email %>
                </td>
                <td>
                    <%= dataTable[i].firstName %>
                </td>
                <td>
                    <%= dataTable[i].lastName %>
                </td>
                <td>
                    <%= dataTable[i].address %>
                </td>
                <td>
                    <a href="/edit-crud?id=<%=dataTable[i].id %>" class="btn-edit" type="button"
                        style="cursor: pointer">edit</a>
                    <a href="/delete-crud?id=<%=dataTable[i].id %>" class="btn-delete" type="button"
                        style="cursor: pointer">delete</a>
                </td>

            </tr>
            <% } %>

    </table>

</body>

</html>